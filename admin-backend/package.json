    "prisma:import-for-render": "ts-node prisma/scripts/import-for-render.ts",
    "prisma:sync-member-levels": "ts-node prisma/scripts/sync-member-levels-to-render.ts",
    "prisma:verify-member-levels": "ts-node prisma/scripts/verify-member-levels-db.ts"
