  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/passport-jwt": "^4.0.0",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
